    "description": "Doctrine2-style entity mapper for Neo4j graph database",
    "keywords": ["neo4j","graph","database","entity-mapper","doctrine"],
    "homepage": "https://github.com/lphuberdeau/Neo4j-PHP-OGM",
    "license": "MIT",
    "authors": [
        {"name": "Louis-Philippe Huberdeau", "email": "lphuberdeau@gmail.com", "homepage": "http://lphuberdeau.com"}
    ],
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/mschnider/neo4jphp"
      }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzlehttp/guzzle": "~4.0",
        "doctrine/common": ">=2.2.0,<2.5.0-dev",
        "mschnider/neo4jphp": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
